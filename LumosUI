		wsheld = true
	end)
	
	RuS.RenderStepped:connect(function(delta)
		if wsheld then
			local MousePos = UIS:GetMouseLocation().X
			local BtnPos = script.Parent.LocalPlayerPanel.WSBar.WSknob.Position
			local SliderSize = script.Parent.LocalPlayerPanel.WSBar.AbsoluteSize.X
			local SliderPos = script.Parent.LocalPlayerPanel.WSBar.AbsolutePosition.X
			local pos = snap((MousePos-SliderPos)/SliderSize,step)
			percentage = math.clamp(pos,0,1)
			script.Parent.LocalPlayerPanel.WSBar.WSknob.Position = UDim2.new(percentage - 0.02 ,0,BtnPos.Y.Scale, BtnPos.Y.Offset)
		end
	end)
	
	
	
	script.Parent.LocalPlayerPanel.WSBar.WSknob.Changed:Connect(function()
		wsenabled = true
		local axis = script.Parent.LocalPlayerPanel.WSBar.WSknob.Position.X.Scale
		local color =  Color3.fromRGB(0, 85, 127):Lerp(Color3.fromRGB(0, 120, 175),axis/1)
		local number = math.floor(axis*100)
		script.Parent.LocalPlayerPanel.WSBar.WSknob.BackgroundColor3 = color
		local volume = number
		local hum = game.Players.LocalPlayer.Character:FindFirstChild("Humanoid")
		if hum and wsenabled then
			if 16 + (WalkSpeedBoostPercent * volume) < 16 then
				hum.WalkSpeed = 16
			else
				hum.WalkSpeed =  16 + (WalkSpeedBoostPercent * volume)
			end
		end
	end)
	UIS.InputEnded:connect(function(input, processed)
		if input.UserInputType == Enum.UserInputType.MouseButton1 then
			wsheld = false
		end
	end)
	
	players.PlayerAdded:Connect(function(Player)
		local IsFriend = Player:IsFriendsWith(UserId)
		if IsFriend then
			game.StarterGui:SetCore("SendNotification",  {
				Title = "Friend";
				Text = Player.Name .. " joined the game";
				Duration = 2;
			})
			playerjoinfx()
		end
	end)
	
	players.PlayerRemoving:Connect(function(Player)
		local IsFriend = Player:IsFriendsWith(UserId)
		if IsFriend then
			game.StarterGui:SetCore("SendNotification",  {
				Title = "Friend";
				Text = Player.Name .. " left the game";
				Duration = 2;
			})
			playerleftfx()()
		end
	end)
	
