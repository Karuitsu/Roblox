			XrayOn(obj)
			script.Parent.LocalPlayerPanel.Xray.BackgroundColor3 = Color3.fromRGB(0, 255, 0)
		elseif script.Parent.LocalPlayerPanel.Xray.BackgroundColor3 == Color3.fromRGB(0, 255, 0) then
			XrayOff(obj)
			script.Parent.LocalPlayerPanel.Xray.BackgroundColor3 = Color3.fromRGB(65, 65, 65)
		end
	end)
	
