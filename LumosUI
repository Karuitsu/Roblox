	script.Parent.toggle.MouseButton1Click:Connect(function()
		Btnsounds()
		if script.Parent.Position == UDim2.new(1.027, -74,0.407, -135) then
			script.Parent:TweenPosition(UDim2.new(1.035, -45,0.398, -135), "Out", "Quint")
			script.Parent.toggle.Text = "<"
		elseif script.Parent.Position == UDim2.new(1.035, -45,0.398, -135) then
			script.Parent:TweenPosition(UDim2.new(1.027, -74,0.407, -135), "Out", "Quint")
			script.Parent.toggle.Text = ">"
		end
	end)
	
