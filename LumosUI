		script.Parent.HomePanels.HubsPanel.BackgroundColor3 = Color3.fromRGB(Rbox.Text , Gbox.Text , Bbox.Text)
		script.Parent.HomePanels.ScriptsPanel.BackgroundColor3 = Color3.fromRGB(Rbox.Text , Gbox.Text , Bbox.Text)
		script.Parent.HomePanels.SettingsPanel.BackgroundColor3 = Color3.fromRGB(Rbox.Text , Gbox.Text , Bbox.Text)
	end)
	
	function zigzag(X) return math.acos(math.cos(X*math.pi))/math.pi end
	
	counter = 0
	
	script.Parent.HomePanels.SettingsPanel.Rainbowbtn.MouseButton1Click:Connect(function()
		if script.Parent.HomePanels.SettingsPanel.Rainbowbtn.BackgroundColor3 == Color3.fromRGB(65, 65, 65) then
			script.Parent.HomePanels.SettingsPanel.Rainbowbtn.BackgroundColor3 = Color3.fromRGB(0, 255, 0)
			repeat
				wait(0.5)
				script.Parent.BackgroundColor3 = Color3.fromHSV(zigzag(counter),1,1)
				script.Parent.ChatPanel.BackgroundColor3 = Color3.fromHSV(zigzag(counter),1,1)
				script.Parent.HomePanels.BackgroundColor3 = Color3.fromHSV(zigzag(counter),1,1)
				script.Parent.LocalPlayerPanel.BackgroundColor3 = Color3.fromHSV(zigzag(counter),1,1)
				script.Parent.HomePanels.GamesPanel.BackgroundColor3 = Color3.fromHSV(zigzag(counter),1,1)
				script.Parent.HomePanels.HubsPanel.BackgroundColor3 = Color3.fromHSV(zigzag(counter),1,1)
				script.Parent.HomePanels.ScriptsPanel.BackgroundColor3 = Color3.fromHSV(zigzag(counter),1,1)
				script.Parent.HomePanels.SettingsPanel.BackgroundColor3 = Color3.fromHSV(zigzag(counter),1,1)
	
				counter = counter + 0.01
			until script.Parent.HomePanels.SettingsPanel.Rainbowbtn.BackgroundColor3 == Color3.fromRGB(65, 65, 65)
		elseif script.Parent.HomePanels.SettingsPanel.Rainbowbtn.BackgroundColor3 == Color3.fromRGB(0, 255, 0) then
			script.Parent.HomePanels.SettingsPanel.Rainbowbtn.BackgroundColor3 = Color3.fromRGB(65, 65, 65)
			wait(0.5)
			script.Parent.BackgroundColor3 = Color3.fromRGB(Rbox.Text , Gbox.Text , Bbox.Text)
			script.Parent.ChatPanel.BackgroundColor3 = Color3.fromRGB(Rbox.Text , Gbox.Text , Bbox.Text)
			script.Parent.HomePanels.BackgroundColor3 = Color3.fromRGB(Rbox.Text , Gbox.Text , Bbox.Text)
			script.Parent.LocalPlayerPanel.BackgroundColor3 = Color3.fromRGB(Rbox.Text , Gbox.Text , Bbox.Text)
			script.Parent.HomePanels.GamesPanel.BackgroundColor3 = Color3.fromRGB(Rbox.Text , Gbox.Text , Bbox.Text)
			script.Parent.HomePanels.HubsPanel.BackgroundColor3 = Color3.fromRGB(Rbox.Text , Gbox.Text , Bbox.Text)
			script.Parent.HomePanels.ScriptsPanel.BackgroundColor3 = Color3.fromRGB(Rbox.Text , Gbox.Text , Bbox.Text)
			script.Parent.HomePanels.SettingsPanel.BackgroundColor3 = Color3.fromRGB(Rbox.Text , Gbox.Text , Bbox.Text)
		end
	end)
	
	
	script.Parent.LocalPlayerPanel.Rejoin.MouseButton1Click:Connect(function()
		script.Parent.LocalPlayerPanel.Rejoin.Text = "Rejoining..."
		RejoinServer()
		wait(3)
		script.Parent.LocalPlayerPanel.Rejoin.Text = "Done!"
		wait(1.5)
		script.Parent.LocalPlayerPanel.Rejoin.Text = "Rejoin"
	end)
	
	script.Parent.LocalPlayerPanel.ServerHop.MouseButton1Click:Connect(function()
		script.Parent.LocalPlayerPanel.ServerHop.Text = "One moment.."
		StartServerHop()
		wait(2)
		script.Parent.LocalPlayerPanel.ServerHop.Text = "Serverhop"
	end)
	
	local clodebounce = false
	script.Parent.HomePanels.ScriptsPanel.ScrollHub.CloVR.MouseButton1Click:Connect(function()
		if not clodebounce then
			clodebounce = true
			loadstring(game:HttpGet("https://pastebin.com/raw/4iTCQwHd", true))()
			clodebounce = false
		end
	end)
	
	
	---end
	function getTime()
		date = os.date("*t")
		return ("%02d:%02d"):format(((date.hour % 24) - 1) % 12 + 1, date.min)
	end
	
	function GetDate()
		local date = {}
		local months = {
			{"January", 31};
			{"February", 28};
			{"March", 31};
			{"April", 30};
			{"May", 31};
			{"June", 30};
			{"July", 31};
			{"August", 31};
			{"September", 30};
			{"October", 31};
			{"November", 30};
			{"December", 31};
		}
		local t = tick()
		date.total = t
		date.seconds = math.floor(t % 60)
		date.minutes = math.floor((t / 60) % 60)
		date.hours = math.floor((t / 60 / 60) % 24)
		date.year = (1970 + math.floor(t / 60 / 60 / 24 / 365.25))
		date.yearShort = tostring(date.year):sub(-2)
		date.isLeapYear = ((date.year % 4) == 0)
		date.isAm = (date.hours < 12)
		date.hoursPm = (date.isAm and date.hours or (date.hours == 12 and 12 or (date.hours - 12)))
		if (date.hoursPm == 0) then date.hoursPm = 12 end
		if (date.isLeapYear) then
			months[2][2] = 29
		end
		do
			date.dayOfYear = math.floor((t / 60 / 60 / 24) % 365.25)
			local dayCount = 0
			for i,month in pairs(months) do
				dayCount = (dayCount + month[2])
				if (dayCount > date.dayOfYear) then
					date.monthWord = month[1]
					date.month = i
	
					date.day = (date.dayOfYear - (dayCount - month[2]) + 1)
					if date.monthWord == "February" then
						date.day = date.day - 1
					end
					break
				end
			end
		end
		function date:format(str)
			str = str
			:gsub("#s", ("%.2i"):format(self.seconds))
			:gsub("#m", ("%.2i"):format(self.minutes))
			:gsub("#h", tostring(self.hours))
			:gsub("#H", tostring(self.hoursPm))
			:gsub("#Y", tostring(self.year))
			:gsub("#y", tostring(self.yearShort))
			:gsub("#a", (self.isAm and "AM" or "PM"))
			:gsub("#W", self.monthWord)
			:gsub("#M", tostring(self.month))
			:gsub("#d", tostring(self.day))
			:gsub("#D", tostring(self.dayOfYear))
			:gsub("#t", tostring(self.total))
			return str
		end
		return date
	end
	
	while true do
		local date = GetDate()
		script.Parent.HomePanels.Date.Text = date:format("#W #d, #Y")
		script.Parent.HomePanels.Date.Time.Text = getTime()
		local friendsonline = #game.Players.LocalPlayer:GetFriendsOnline()
		wait(1)
		if friendsonline == 1 then
			script.Parent.HomePanels.FriendsPanel.FriendsOnlineAmount.Text = "You have "..tostring(friendsonline).." friend online"
		elseif friendsonline == 0 then
			script.Parent.HomePanels.FriendsPanel.FriendsOnlineAmount.Text = "You have no friends online"
		else
			script.Parent.HomePanels.FriendsPanel.FriendsOnlineAmount.Text = "You have "..tostring(friendsonline).." friends online"
		end
	end
	
	for _, Player in ipairs(game.Players:GetChildren()) do
		if game.Players.LocalPlayer:IsFriendsWith(Player.UserId) then
			friendsingame = friendsingame - 1
			script.Parent.HomePanels.FriendsPanel.FriendsIngameAmount.Text = friendsingame.." are in this game"	
		end
		if friendsingame == 0 then
			script.Parent.HomePanels.FriendsPanel.FriendsIngameAmount.Text = "None are in this game"	
		end
	end
	---end
end
coroutine.wrap(TPXLVT_fake_script)()
