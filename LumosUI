	function OpenHomePanel()
		local blur = Instance.new("BlurEffect")
		blur.Parent = game.Lighting
		blur.Size = 1
		blur.Name = "quickbro"
		game:GetService'Workspace'.Camera.FieldOfView = 70
		blur.Size = 3
		wait(0.01)
		game:GetService'Workspace'.Camera.FieldOfView = 65
		blur.Size = 5
		wait(0.01)
		game:GetService'Workspace'.Camera.FieldOfView = 60
		blur.Size = 6
		wait(0.01)
		game:GetService'Workspace'.Camera.FieldOfView = 55
		blur.Size = 8
		wait(0.01)
		game:GetService'Workspace'.Camera.FieldOfView = 50
		blur.Size = 10
		wait(0.01)
		script.Parent.HomePanels.Visible = true
		local S = Instance.new("Sound")
		S.Parent = game:GetService("StarterGui")
		S.Name = "slide"
		S.SoundId = 'rbxassetid://2050305749'
		S.Volume = 0.5
		S.PlaybackSpeed = 0.9
		S.PlayOnRemove = true
		S:Destroy()
		script.Parent.HomePanels:TweenSize(UDim2.new(0, 905,0, 70), "Out", "Quint", 1)
		
		repeat
			script.Parent.HomePanels.Thumbnail.ImageTransparency = script.Parent.HomePanels.Thumbnail.ImageTransparency - 0.05;
			wait(0.001);		
		until script.Parent.HomePanels.Thumbnail.ImageTransparency <= 0;
		
		script.Parent.HomePanels.SettingsPanel.Visible = true
		script.Parent.HomePanels.SettingsPanel:TweenSize(UDim2.new(0, 287,0, 168), "Out", "Quint", 2.5)
		script.Parent.HomePanels.FriendsPanel.Visible = true
		script.Parent.HomePanels.FriendsPanel:TweenSize(UDim2.new(0, 287,0, 78), "Out", "Quint", 2.5)
		
		repeat
			script.Parent.HomePanels.Thumbnail.WelcomingText.TextTransparency = script.Parent.HomePanels.Thumbnail.WelcomingText.TextTransparency - 0.05;
			wait(0.001);
		until script.Parent.HomePanels.Thumbnail.WelcomingText.TextTransparency <= 0;
		
		script.Parent.HomePanels.SettingsPanel.Rainbowbtn.Visible = true
		script.Parent.HomePanels.SettingsPanel.Changebtn.Visible = true
		script.Parent.HomePanels.SettingsPanel.R.Visible = true
		script.Parent.HomePanels.SettingsPanel.G.Visible = true
		script.Parent.HomePanels.SettingsPanel.B.Visible = true
		script.Parent.HomePanels.SettingsPanel.Icon.Visible = true
		script.Parent.HomePanels.SettingsPanel.Icon2.Visible = true
		script.Parent.HomePanels.SettingsPanel.Title.Visible = true
		script.Parent.HomePanels.FriendsPanel.FriendsIngameAmount.Visible = true
		script.Parent.HomePanels.FriendsPanel.FriendsOnlineAmount.Visible = true
		script.Parent.HomePanels.FriendsPanel.Title.Visible = true
		script.Parent.HomePanels.FriendsPanel.Icon.Visible = true
		script.Parent.HomePanels.FriendsPanel.Icon2.Visible = true
	
		repeat
			script.Parent.HomePanels.Thumbnail.WelcomingText.User.TextTransparency = script.Parent.HomePanels.Thumbnail.WelcomingText.User.TextTransparency - 0.05;
			wait(0.001);
		until script.Parent.HomePanels.Thumbnail.WelcomingText.User.TextTransparency <= 0;
		
		repeat
			script.Parent.HomePanels.Date.TextTransparency = script.Parent.HomePanels.Date.TextTransparency - 0.05;
			script.Parent.HomePanels.Date.Time.TextTransparency = script.Parent.HomePanels.Date.Time.TextTransparency - 0.05;
			wait(0.001);
		until script.Parent.HomePanels.Date.TextTransparency <= 0 and script.Parent.HomePanels.Date.Time.TextTransparency <= 0;
	end
	
	function CloseHomePanel()
		
		repeat
			script.Parent.HomePanels.Date.Time.TextTransparency = script.Parent.HomePanels.Date.Time.TextTransparency + 0.05;
			script.Parent.HomePanels.Date.TextTransparency = script.Parent.HomePanels.Date.TextTransparency + 0.05;
			wait(0.0001);
		until script.Parent.HomePanels.Date.Time.TextTransparency >= 1 and script.Parent.HomePanels.Date.TextTransparency >= 1;
		
		script.Parent.HomePanels.FriendsPanel.FriendsIngameAmount.Visible = false
		script.Parent.HomePanels.FriendsPanel.FriendsOnlineAmount.Visible = false
		script.Parent.HomePanels.FriendsPanel.Title.Visible = false
		script.Parent.HomePanels.FriendsPanel.Icon.Visible = false
		script.Parent.HomePanels.FriendsPanel.Icon2.Visible = false
		script.Parent.HomePanels.SettingsPanel.Rainbowbtn.Visible = false
		script.Parent.HomePanels.SettingsPanel.Changebtn.Visible = false
		script.Parent.HomePanels.SettingsPanel.R.Visible = false
		script.Parent.HomePanels.SettingsPanel.G.Visible = false
		script.Parent.HomePanels.SettingsPanel.B.Visible = false
		script.Parent.HomePanels.SettingsPanel.Title.Visible = false
		script.Parent.HomePanels.SettingsPanel.Icon.Visible = false
		script.Parent.HomePanels.SettingsPanel.Icon2.Visible = false
		
		script.Parent.HomePanels.SettingsPanel:TweenSize(UDim2.new(0, 287,0, 11), "Out", "Quint", 2.5)
		script.Parent.HomePanels.FriendsPanel:TweenSize(UDim2.new(0, 287,0, 0), "Out", "Quint", 2.5)
		
		repeat
			script.Parent.HomePanels.Thumbnail.WelcomingText.User.TextTransparency = script.Parent.HomePanels.Thumbnail.WelcomingText.User.TextTransparency + 0.05;
			wait(0.0001);
		until script.Parent.HomePanels.Thumbnail.WelcomingText.User.TextTransparency >= 1;
		
		repeat
			script.Parent.HomePanels.Thumbnail.WelcomingText.TextTransparency = script.Parent.HomePanels.Thumbnail.WelcomingText.TextTransparency + 0.05;
			wait(0.0001);
		until script.Parent.HomePanels.Thumbnail.WelcomingText.TextTransparency >= 1;
		
		script.Parent.HomePanels.SettingsPanel.Visible = false
		script.Parent.HomePanels.FriendsPanel.Visible = false
	
		repeat
			script.Parent.HomePanels.Thumbnail.ImageTransparency = script.Parent.HomePanels.Thumbnail.ImageTransparency + 0.05;
			wait(0.0001);
		until script.Parent.HomePanels.Thumbnail.ImageTransparency >= 1;
	
		local S = Instance.new("Sound")
		S.Parent = game:GetService("StarterGui")
		S.Name = "slide"
		S.SoundId = 'rbxassetid://2050305749'
		S.Volume = 0.5
		S.PlaybackSpeed = 0.9
		S.PlayOnRemove = true
		S:Destroy()
		script.Parent.HomePanels:TweenSize(UDim2.new(0, 1,0, 70), "Out", "Quint", 1)
		game:GetService'Workspace'.Camera.FieldOfView = 50
		game.Lighting:FindFirstChild('quickbro').Size = 8
		wait(0.01)
		game:GetService'Workspace'.Camera.FieldOfView = 55
		game.Lighting:FindFirstChild('quickbro').Size = 6
		wait(0.01)
		game:GetService'Workspace'.Camera.FieldOfView = 60
		game.Lighting:FindFirstChild('quickbro').Size = 5
		wait(0.01)
		game:GetService'Workspace'.Camera.FieldOfView = 65
		game.Lighting:FindFirstChild('quickbro').Size = 3
		wait(0.01)
		game:GetService'Workspace'.Camera.FieldOfView = 70
		game.Lighting:FindFirstChild('quickbro').Size = 1
		game.Lighting:FindFirstChild('quickbro'):Destroy()
		wait(0.5)
		script.Parent.HomePanels.Visible = false
	end
	
	
	function OpenChatPanel()
		script.Parent.ChatPanel.Visible = true
		script.Parent.ChatPanel:TweenSize(UDim2.new(0, 200,0, 314), "Out", "Quint", 1)
		wait(0.5)
		script.Parent.ChatPanel.Emojiartbtn.Visible = true
		script.Parent.ChatPanel.Emojibtn.Visible = true
		script.Parent.ChatPanel.Cursesbtn.Visible = true
	end
	
	function CloseChatPanel()
	if script.Parent.ChatPanel.EmojiArtsPanel.Visible == true or script.Parent.ChatPanel.EmojiPanel.Visible == true or script.Parent.ChatPanel.CursesPanel.Visible == true or script.Parent.ChatPanel.EmojiArtsPanel.Visible == false or script.Parent.ChatPanel.EmojiPanel.Visible == false or script.Parent.ChatPanel.CursesPanel.Visible == false then
		script.Parent.ChatPanel.EmojiArtsPanel.Visible = false
		script.Parent.ChatPanel.EmojiPanel.Visible = false
		script.Parent.ChatPanel.CursesPanel.Visible = false
		script.Parent.ChatPanel.Emojiartbtn.Visible = false
		script.Parent.ChatPanel.Emojibtn.Visible = false
		script.Parent.ChatPanel.Cursesbtn.Visible = false
		script.Parent.ChatPanel:TweenSize(UDim2.new(0, 200,0, 0), "Out", "Quint", 1)
		wait(0.5)
		script.Parent.ChatPanel.Visible = false
		end
	end
	
	
