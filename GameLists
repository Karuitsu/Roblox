repeat wait() until game ~= nil
repeat wait() until #game:GetChildren() > 70
repeat wait() until game:GetService('Players') ~= nil
repeat wait() until game:GetService('Players').LocalPlayer ~= nil
repeat wait() until game.gameId ~= nil and game.gameId ~= 0

if game.PlaceId == 5907259314 then ---Boss Brawl
	local Notification = loadstring(game:HttpGet("https://raw.githubusercontent.com/Karuitsu/Roblox/main/NotificationButtons"))()

	spawn(function()
		Notification.Notify("GameDetected!", "Execute?", "https://raw.githubusercontent.com/xunniez/Simulator/main/BossBrawlScript");
	end)


elseif game.PlaceId == 606849621 then ---Jailbreak
		local Notification = loadstring(game:HttpGet("https://raw.githubusercontent.com/Karuitsu/Roblox/main/NotificationButtons"))()

	spawn(function()
		Notification.Notify("GameDetected!", "Execute?", "https://raw.githubusercontent.com/Karuitsu/Roblox/main/JBScripts");
	end)

elseif game.PlaceId == 2753915549 then ---Blox Fruits world1
	local Notification = loadstring(game:HttpGet("https://raw.githubusercontent.com/Karuitsu/Roblox/main/NotificationButtons"))()

	spawn(function()
		Notification.Notify("GameDetected!", "Execute?", "https://raw.githubusercontent.com/SenseiJoshy/CastAways/main/Main/CastAways");
	end)

end
